{
  "name": "my-boilerpalte",
  "author": "kinoko109 <tatsuaki1009@gmail.com>",
  "repository": "git@github.com.kinoko109:kinoko109/my-boilerplate.git",
  "main": "index.js",
  "scripts": {
    "check-all": "npm-run-all spell-check type-check",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./{src,mock}/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache",
    "fix-ts-by-prettier": "prettier \"./{src,mock}/**/*.{ts,tsx}\" --write --cache",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:cspell": "node ../../scripts/lint-cspell-dictionary/index.js -f",
    "fix:css": "stylelint \"./src/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache --cache-strategy content",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p lint:*",
    "lint:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:cspell": "node ../../scripts/lint-cspell-dictionary/index.js",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --cache --cache-strategy content",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "spell-check": "cspell \"./{src,mock}/**/*.{ts,tsx}\" --no-progress",
    "test": "jest --coverage",
    "type-check": "tsc",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "styled-components": "^5.3.9",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@swc/core": "^1.3.41",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "cspell": "^6.30.1",
    "editorconfig-checker": "^5.0.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-strict-dependencies": "^1.0.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^46.0.0",
    "fixpack": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "stylelint": "^15.3.0",
    "typescript": "^5.0.2"
  },
  "version": "1.0.0",
  "license": "MIT"
}
